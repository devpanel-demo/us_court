{%
  set classes = [
    'block',
    'block-' ~ configuration.provider|clean_class,
    'block-' ~ plugin_id|clean_class,
  ]
%}

{% embed 'uscgov:details' with {
  attributes,
  classes,
  title_prefix,
  title_suffix,
  title_attributes,
  label,
  content,
} %}
  {% block details_content %}
    {{ content }}
  {% endblock %}
{% endembed %}
