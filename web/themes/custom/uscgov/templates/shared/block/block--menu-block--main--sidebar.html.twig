{% set heading_id = attributes.id ~ '-menu'|clean_unique_id %}
<nav class="side-nav" aria-labelledby="{{ heading_id }}"{{ attributes|without('role', 'aria-labelledby') }}>
  {{ title_prefix }}
  <h2{{ title_attributes.addClass('side-nav__title').setAttribute('id', heading_id) }}>{{ configuration.label }}</h2>
  {{ title_suffix }}
  {% block content %}
    {{ content }}
  {% endblock %}
</nav>
